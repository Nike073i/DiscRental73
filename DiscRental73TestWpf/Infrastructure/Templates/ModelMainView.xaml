<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:DiscRental73TestWpf.ViewModels.ManagementViewModels"
                    xmlns:view="clr-namespace:DiscRental73TestWpf.Views.ManagementViews">
    <DataTemplate DataType="{x:Type vm:CdDiscManagementViewModel}">
        <view:CdDiscManagementView DataContext="{Binding}"/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:DvdDiscManagementViewModel}">
        <view:DvdDiscManagementView DataContext="{Binding}"/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:BluRayDiscManagementViewModel}">
        <view:BluRayDiscManagementView DataContext="{Binding}"/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:ClientManagementViewModel}">
        <view:ClientManagementView DataContext="{Binding}"/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:EmployeeManagementViewModel}">
        <view:EmployeeManagementView DataContext="{Binding}"/>
    </DataTemplate>
</ResourceDictionary>