<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:evm="clr-namespace:DiscRental73.Wpf.ViewModels.EntityViewModels"
                    xmlns:eev="clr-namespace:DiscRental73.Wpf.Views.EntityEditViews">
    <DataTemplate DataType="{x:Type evm:BluRayDiscViewModel}">
        <eev:BluRayDiscEditView DataContext="{Binding}"/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type evm:CdDiscViewModel}">
        <eev:CdDiscEditView DataContext="{Binding}"/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type evm:DvdDiscViewModel}">
        <eev:DvdDiscEditView DataContext="{Binding}"/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type evm:ClientViewModel}">
        <eev:ClientEditView DataContext="{Binding}"/>
    </DataTemplate>
</ResourceDictionary>